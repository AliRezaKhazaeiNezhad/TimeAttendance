<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta name="description" content="تم مدیریتی راستچین" />
    <meta name="author" content="Ali Reza Khazaei Nezhad" />
    <title>@(ViewBag.Title)</title>

    @Styles.Render("~/Content/Theme2")

    <link rel="shortcut icon" href="~/wwwroot/dashboard/img/128.png" />
</head>


<body class="page-header-fixed sidemenu-closed-hidelogo page-content-white page-md sidemenu-container-reversed header-white white-sidebar-color logo-indigo" style="overflow-x:hidden">
    @Html.Partial("_Loading")
    <div class="page-wrapper">
        @Html.Partial("_HorizontalMenu")
        <div class="page-container">
            <div class="sidebar-container">
                <div class="sidemenu-container navbar-collapse collapse fixed-menu">
                    <div id="remove-scroll" class="left-sidemenu">
                        <ul class="sidemenu  page-header-fixed" data-keep-expanded="false" data-auto-scroll="true"
                            data-slide-speed="200" style="padding-top: 20px">
                            @Html.Partial("_VerticalMenu")
                        </ul>
                    </div>
                </div>
            </div>
            <div class="page-content-wrapper">
                <div class="page-content">
                    <div class="page-bar">
                        <div class="page-title-breadcrumb">
                        </div>
                    </div>
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <script src=></script>
    @Scripts.Render("~/bundles/Theme2")
    @RenderSection("scripts", required: false)
    <script>
        ReomveLoading();
    </script>
</body>


